{
    "name": "jtc-solutions/helpers",
    "description": "Group of static helpers for type operations.",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "JtcSolutions\\Helpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JtcSolutions\\Helpers\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "David Jungman",
            "email": "david@jtc-solutions.cz"
        }
    ],
    "require": {
        "ramsey/uuid": "^4.7",
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "phpstan/phpstan": "^2.1",
        "grifart/phpstan-oneline": "^0.5.0",
        "symplify/easy-coding-standard": "^12.1",
        "slevomat/coding-standard": "^8.16"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
